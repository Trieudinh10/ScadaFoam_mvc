  <body>
    <%- include('header.ejs'); -%>
    <div class="operate_container">
      <div class="operate_container-title">
        <h2>Chế độ thủ công</h2>
        <img id="Trang_thai_manual" class="den_cd" />
        <script>
          fn_SymbolStatus("Trang_thai_manual", "Den_xanh", "Den_manual");
        </script>
      </div>
      <div class="block_start_stop">
        <div class="start_stop-btn-start">
          <button class="btn_on" id="btn_on_tn1" onclick="socket.emit('cmd_start_manual', true);">START</button>
          <img id="Trang_thai_manual" class="den_cd" /><script>fn_SymbolStatus("Trang_thai_manual", "Den_xanh", "Den_manual");</script>
        </div>
        <div class="start_stop-btn-save">
          <button class="btt_sua_luu" id="btt_Main_Save"onclick="fn_Main_SaveBtt()">LƯU</button>
          <button class="btt_sua_luu" id="btt_Main_Edit"onclick="fn_Main_EditBtt()">SỬA</button>
        </div>
      </div>

      <div class="operate_row">
        <div class="setting_item">
          <h2 id="dieuhoa1" class="setting_item-title">Điều Hòa 1</h2>
          <div class="setting_content-item">
            <div class="setting_on">
              <button class="setting_on-btn">Khởi động</button>
            </div>
            <div class="setting_off">
              <button class="setting_on-btn">Dừng</button>
            </div>
          </div>
          <div class="setting_content-item">
            <h2>Chế độ (mod)</h2>
            <div class="setting_input">
              <label for="">Cài đặt</label>
              <select id="mode_man_2" disabled="disabled" type="text">
                <option value="0">0</option>
                <option value="1">1</option>
                <option value="2">2</option>
                <option value="3">3</option>
                <option value="4">4</option>
              </select>
              <script>fn_Main_IOField_IO("Mode_manu_2", "mode_man_2", 0);</script>
            </div>
          </div>
          <div class="setting_content-item">
            <h2>Tốc độ Quạt (mod)</h2>
            <div class="setting_input">
              <label for="">Cài đặt</label>
              <select id="speed_manu_2" disabled="disabled" type="text">
                <option value="0">0</option>
                <option value="1">1</option>
                <option value="2">2</option>
                <option value="3">3</option>
              </select>
              <script>fn_Main_IOField_IO("Fan_speed_manu_2", "speed_manu_2", 0);</script>
            </div>
          </div>
          <div class="setting_content-item">
            <h2>Góc mở (mod)</h2>
            <div class="setting_input">
              <label for="">Cài đặt</label>
              <select id="position_manu_2" disabled="disabled" type="text">
                <option value="1">1</option>
                <option value="2">2</option>
                <option value="3">3</option>
                <option value="4">4</option>
                <option value="5">5</option>
                <option value="10">10</option>
              </select>
              <script>
                fn_Main_IOField_IO("Van_position_manu_2", "position_manu_2", 0);
              </script>
            </div>
          </div>
          <div class="setting_content-item">
            <h2>Nhiệt độ (&deg;C)</h2>
            <div class="setting_input">
              <label for="">Cài đặt</label>
              <input id="temp_manu_2" disabled="disabled" type="number" min="16" max="31"/>
              <script>fn_Main_IOField_IO("Nhap_temp_manu_2", "temp_manu_2", 0);</script>
            </div>
          </div>
        </div>


